{
  "name": "web-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --config-precedence file-override --write",
    "prettier-all": "prettier --loglevel error --config-precedence file-override --write ./**/*.{ts,js,vue,scss,css}",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "gh-deploy": "bash ./gh-pages-deploy.sh",
    "s3-deploy": "aws s3 sync dist s3://huxingongyi.com"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.0",
    "element-ui": "^2.8.2",
    "lodash-es": "^4.17.11",
    "luxon": "^1.15.0",
    "nprogress": "^0.2.0",
    "verification-code": "^1.0.0",
    "vue": "^2.6.10",
    "vue-clamp": "^0.2.1",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "vuex-persistedstate": "^2.5.4",
    "workbox-window": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/luxon": "^1.13.0",
    "@types/nprogress": "^0.0.29",
    "@types/workbox-sw": "^4.0.0",
    "@types/workbox-window": "^4.3.0",
    "@vue/cli-plugin-babel": "^4.0.0-alpha.1",
    "@vue/cli-plugin-pwa": "^4.0.0-alpha.1",
    "@vue/cli-plugin-typescript": "^4.0.0-alpha.1",
    "@vue/cli-plugin-unit-jest": "^4.0.0-alpha.1",
    "@vue/cli-service": "^4.0.0-alpha.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.9.2",
    "prettier": "^1.17.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier",
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "prettier",
      "vue-cli-service lint",
      "git add"
    ],
    "*.scss": [
      "prettier",
      "git add"
    ]
  }
}
